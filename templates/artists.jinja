{% extends 'base.jinja' %}

{% block content %}
    <h1>{% block title %} Welcome to ComicWorld {% endblock %}</h1>
    {% for artist in artists %}
        <a href="{{ url_for('artistpage', artist=artist[0]) }}">
            <h2>{{ artist[0] }}</h2>
        </a>
        <hr>
    {% endfor %}
{% endblock %}